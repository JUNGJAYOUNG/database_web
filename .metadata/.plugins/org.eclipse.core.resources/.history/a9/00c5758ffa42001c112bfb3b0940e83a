package codingTest;
import java.util.*;

public class MaxNumberRule {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		//N, M, K를 공백을 기준으로 구분하여 입력받기
		int n = sc.nextInt();
		int m = sc.nextInt();
		int k = sc.nextInt();
		
		//N개의 수를 공백을 기준으로 구분하여 입력받기
		int []arr = new int[n];
		for(int i=0;i<n;i++) {
			arr[i] = sc.nextInt();
		}
		
		
	}

}
